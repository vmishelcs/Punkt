main {
  const expressions = [
    (-11 // 19) - (35 // 16) + (-1 // 6),
    (16 // 19) - (14 // 9) + (-19 // 12),
    (-24 // 19) - (-3 // 5) - (-3 // 4),
    (11 // 14) + (-53 // 20) - (7 // 19),
    (16 // 19) + (1 // 2) + (5 // 19),
    (31 // 12) * (15 // 13) * (-10 // 9),
    (-26 // 17) * (-7 // 10) / (5 // 3),
    (-23 // 12) + (-12 // 5) + (11 // 6),
    (-11 // 10) / (-1 // 4) * (-40 // 19),
    (-5 // 2) / (13 // 7) * (9 // 7)
  ].

  const answers = [
    -2675 // 912,
    -1571 // 684,
    33 // 380,
    -5939 // 2660,
    61 // 38,
    -775 // 234,
    273 // 425,
    -149 // 60,
    -176 // 19,
    -45 // 26
  ].

  const n = sizeof(expressions).

  for var i = 0, i < n, i += 1 {
    print i + 1, ": ".
    if expressions[i] == answers[i] {
      println "correct".
    } else {
      println "wrong".
    }
  }
}
