main {
    const x = 2.

    const a = 2.
    if x == a {
        println "x = ", a.
    } else {
        println "x != ", a.
    }
    
    const b = 4.
    if x + 2 == b {
        println "x + 2 = ", b.
    } else {
        println "x + 2 != ", b.
    }

    const c = 4.
    if x * 2 == c {
        println "x * 2 = ", c.
    } else {
        println "x * 2 != ", c.
    }
}
